{
  "name": "workflow",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "start": "chmod +x /app/start.sh && /app/start.sh",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:seed": "node prisma/seed.js",
    "db:update": "npx prisma migrate dev --name \"`date +%Y%m%d%H%M%S`\"",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "@quasar/extras": "^1.17.0",
    "csv-parse": "^5.6.0",
    "nuxt": "^4.2.1",
    "nuxt-quasar-ui": "^3.0.0",
    "pg-copy-streams": "^7.0.0",
    "quasar": "^2.18.6",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "prisma": "^6.19.2",
    "sass-embedded": "^1.97.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
