{
  "name": "workflow",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "npm install && npm run db:generate && npm run db:migrate && npm run db:seed && npx nuxt dev",
    "start": "node .output/server/index.mjs",
    "generate": "npx nuxt generate",
    "preview": "npx nuxt preview",
    "postinstall": "npx nuxt prepare",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:update": "npx prisma migrate dev --name \"`date +%Y%m%d%H%M%S`\"",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "@quasar/extras": "^1.17.0",
    "csv-parse": "^5.6.0",
    "nuxt": "^4.2.1",
    "nuxt-quasar-ui": "^3.0.0",
    "pg-copy-streams": "^7.0.0",
    "quasar": "^2.18.6",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "prisma": "^6.19.2",
    "sass-embedded": "^1.97.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node prisma/seed.js"
  }
}
